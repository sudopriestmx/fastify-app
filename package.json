{
  "name": "fastify-app",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "pretest": "npm run lint",
    "test": "tap --before=test/run-before.js \"test/**/*.test.js\" --after=test/run-after.js",
    "test:nostop": "tap --before=test/run-before.js \"test/**/*.test.js\"",
    "start": "fastify start --options app.js",
    "dev": "npm run start -- --watch -debug",
    "mongo:start": "docker run -d -p 27017:27017 --rm --name fastify-mongo mongo:6",
    "mongo:stop": "docker container stop fastify-mongo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/env": "^4.0.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/mongodb": "^6.0.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/sensible": "^5.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "csv-parse": "^5.5.5",
    "csv-stringify": "^6.4.6",
    "fastify": "^4.2.0",
    "fastify-cli": "^4.2.0",
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "dockerode": "^4.0.2",
    "pino-pretty": "^10.3.1",
    "standard": "^16.0.4",
    "tap": "16.3.2"
  }
}
